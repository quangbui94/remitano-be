{
  "extends": "@tsconfig/node16/tsconfig.json",
  "ts-node": {
    "require": ["tsconfig-paths/register"],
    "files": true
  },
  "compilerOptions": {
    "types": ["node"],
    "baseUrl": "src",
    "noImplicitAny": false,
  },
  "include": ["src"],
  "exclude": ["node_modules"],
  "target": "es6",
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
}
